<% for week in @weeks %>
<% if week.to_i == @week_nav %>
<b><%= week.to_date.to_s(:long) %></b>
<% else %>
<%= link_to week.to_date.to_s(:long), :action => :weekly_report, :id => week.to_i %>
<% end%>
<% unless week == @weeks.last %> | <% end %>
<% end %>
<p/>

<%= br_separated @report %>
